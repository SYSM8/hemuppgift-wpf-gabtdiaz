<Window x:Class="Hemuppgift_WPFtemplate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="MainFrame" />
        <Button x:Name="btn1" 
                Content="Grid" 
                Click="NavigateToGrid"/>
        <Button x:Name="btn2" 
                Content="StackPanel" 
                Grid.Column="1"/>
        <Button x:Name="btn3" 
                Content="WrapPanel" 
                Grid.Row="1"/>
        <Button x:Name="btn4" 
                Content="DockPanel" 
                Grid.Column="1" 
                Grid.Row="1"/>
    </Grid>
</Window>
